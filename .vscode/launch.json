{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "IOS/LINUX: Run main",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "python": "${workspaceFolder}/stock_venv/bin/python",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },

        {
            "name": "WINDOWS: Run main",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "python": "${workspaceFolder}\\stock_venv\\Scripts\\python.exe",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
